{
  "name": "mobilebenchmark",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "androidDev": "cross-env APP=true react-native webpack-start",
    "webDev": "cross-env APP=false webpack-dev-server --mode=development --open --hot",
    "androidMetroDev": "react-native start --reset-cache",
    "forwardPorts": "adb -d reverse tcp:8081 tcp:8081",
    "androidProd": "cross-env APP=true react-native webpack-bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "androidMetroProd": "react-native bundle --verbose --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/metro/index.android.bundle --assets-dest android/app/src/main/res/metro/",
    "webProd": "webpack --mode=production"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/cli": "^6.3.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "axios": "0.24.0",
    "babel-plugin-lodash": "^3.3.4",
    "immutable": "^4.0.0",
    "lodash": "4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.66.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-startup-time": "^2.0.0",
    "react-native-video": "^5.2.0",
    "react-native-web": "0.17.5",
    "react-refresh": "^0.11.0",
    "reactotron-react-native": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@babel/runtime": "7.12.5",
    "@callstack/repack": "2.5.1",
    "@react-native-community/cli-platform-android": "6.3.0",
    "@types/lodash": "4.14.178",
    "@types/react-dom": "^17.0.11",
    "@types/react-native": "0.66.4",
    "babel-loader": "8.2.3",
    "babel-plugin-react-native-web": "0.17.5",
    "cross-env": "7.0.3",
    "html-webpack-plugin": "5.5.0",
    "metro-react-native-babel-preset": "0.66.2",
    "terser-webpack-plugin": "5.2.5",
    "typescript": "4.4.4",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0"
  },
  "resolutions": {
    "@types/react": "17"
  }
}
